/* pages/other/other.wxss */

.main {
  width: 100vw;
  height: 100vh;
  position: flex;
  justify-content: center;
  align-items: center;
}

.btns {
  display: flex;
  flex-direction: column;
  margin-top: 250rpx;


}

.btn {
  width: 70%;
  border-radius: 98rpx;
  font-size: 36rpx;
  margin-top: 30rpx;
  color: #fff;
}
.btn1 {
  background: #291bf1;
}
.btn2 {
  background: rgba(255, 0, 43, 0.596);
}
.btn3 {
  background: rgba(255, 0, 0);
}
.btn4 {
  background: rgba(0, 255, 157, 0.596);
}
.img {
  position: absolute;
  left: 0;
  top: 0;
  z-index: -9;
  width: 100vw;
  height: 100vh;
}
image {
  width: 100%;
  height: 100%;
}